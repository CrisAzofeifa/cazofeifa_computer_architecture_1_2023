module RegisterFileTest;

  reg [2:0] readAddr1, readAddr2, writeAddr;
  reg writeEnable;
  reg [31:0] writeData;
  wire [31:0] readData1, readData2;
  
  RegisterFile dut (
    .readAddr1(readAddr1),
    .readAddr2(readAddr2),
    .writeAddr(writeAddr),
    .writeEnable(writeEnable),
    .writeData(writeData),
    .readData1(readData1),
    .readData2(readData2)
  );

  initial begin
    $monitor("Time = %0t: ReadData1 = %0d, ReadData2 = %0d", $time, readData1, readData2);
    
    // Inicialización de registros
    dut.registers[0] = 10;
    dut.registers[1] = 20;
    dut.registers[2] = 30;
    dut.registers[3] = 40;
    dut.registers[4] = 50;
    dut.registers[5] = 60;
    dut.registers[6] = 70;
    dut.registers[7] = 80;
    dut.registers[8] = 90;
    
    // Test de lectura
    readAddr1 = 1;
    readAddr2 = 3;
    #10;
    
    // Test de escritura
    writeAddr = 5;
    writeEnable = 1;
    writeData = 100;
    #10;
    
    // Test de lectura después de escritura
    readAddr1 = 5;
    readAddr2 = 2;
    writeEnable = 0;
    #10;
    
    $finish;
  end

endmodule

#!/bin/bash
# this file will be overwritten by the project setup script
SIMICS_BASE_PACKAGE="/home/mendezos/Documents/simics_installation/simics-6.0.166"
export SIMICS_BASE_PACKAGE
if [ -f "/home/mendezos/Documents/simics-project/.package-list" ]; then
    exec "/home/mendezos/Documents/simics_installation/simics-6.0.166/bin/checkpoint-merge" --package-list "/home/mendezos/Documents/simics-project/.package-list" --project "/home/mendezos/Documents/simics-project" "$@"
else
    exec "/home/mendezos/Documents/simics_installation/simics-6.0.166/bin/checkpoint-merge" --project "/home/mendezos/Documents/simics-project" "$@"
fi
